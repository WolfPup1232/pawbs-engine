{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Pawbs Engine",
  "version": "0.1.0",
  "identifier": "com.pawbs-engine.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "title": "Pawbs Engine",
        "width": 1366,
        "height": 768,
        "resizable": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
